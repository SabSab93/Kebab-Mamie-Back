// prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Kebab {
  id          Int    @id @default(autoincrement())
  name        String
  description String @map("desc")
  price       Float
  img         String
}
